SELECT *
  FROM recent_grads;
  
  SELECT * 
  FROM recent_grads;
select * from recent_grads;

SELECT *
  FROM recent_grads
 LIMIT 5;
 
 SELECT Major, ShareWomen  
  FROM recent_grads;
  
  SELECT Major, ShareWomen  
  FROM recent_grads 
 WHERE ShareWomen < 0.5;
 
 SELECT Major, Major_category, Median, ShareWomen 
  FROM recent_grads 
 WHERE ShareWomen > 0.5 
   AND Median > 50000;
